$(function(){var b=$("html")[0].lang;$.ajaxDirect({type:"GET",url:urlMap.apnsetting.step1,loadingType:"0",afterFn:function(a){"zh"==b?($(document).attr("title",a[1].apnTitle),a=template("apnSettings1_zh",a[1]),document.getElementById("content1_zh").innerHTML=a):"ja"==b&&($(document).attr("title",a[0].apnTitle),a=template("apnSettings1_jp",a[0]),document.getElementById("content1_jp").innerHTML=a)}});if(!isDesktop()){var c=document.querySelectorAll(".img_responsive");[].forEach.call(c,function(a){a.src=a.src.replace("desktop","mobile")})}});