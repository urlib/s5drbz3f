    <script type="text/javascript">
        function postdatas() {
            //处理页面帮助问题
            var isResolved;
            var reason = $('#reason').val();
            if ($(':radio[@checked]').val() == 1) {
                isResolved = 1;
            }
            else {

                if (reason == 0 || reason == undefined) {
                    alert('请选择原因!');
                    return;
                }
                isResolved = 0;
            }
            return jQuery.ajax(
                {
                    url: "SaveEvaluation",
                    dataType: 'json',
                    type: "POST",
                    async: true,
                    data: {IsResolved: isResolved, DocumentId: documentId, Reason: reason },
                    beforeSend: function () {
                        $('.solve_feedback').hide();
                        $('#resolvedEnd').show();
                        $('#resolvedEnd').html("<div  style='width:100%;padding:30px 0;text-align:center;'><img src='/static-res/img/service/loading.gif?186205939'/></div>");
                    },
                    success: function (data) {
                            $('#resolvedEnd').html("<dt>您的反馈信息已成功提交，谢谢您的反馈。</dt>");
                    }
                }
            );
        }

    </script>
    
