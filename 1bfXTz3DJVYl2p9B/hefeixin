 ($('#vidoDiv').width() + 10) / 2;            $('#vidoDiv').css({                'margin-top': -popuptopmargin,                'margin-left': -popupleftmargin,            });            //$("#vidoDiv").fadeIn();            $("#vidoDiv").show();        });        $('#closeBtn').on("click", function () {            /*$('#mask_layer , #vidoDiv').fadeOut();*/            $('#mask_layer , #vidoDiv').hide();            var Media = document.getElementById("videoEle");            Media.pause(); //暂停        });        // 专业服务支持，移动品牌背书        $("#orderBtn").on("mouseenter", function () {            $(".expert-qrcode").show();        })        $("#orderBtn").on("mouseleave", function () {            $(".expert-qrcode").hide();        })    });</script>

